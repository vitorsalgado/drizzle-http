{
  "name": "@drizzle-http/monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn test --colors --coverage --ci",
    "test:failures": "yar test --onlyFailures",
    "prepare": "husky install",
    "build": "yarn workspaces foreach run build",
    "lint": "eslint --ext .ts .",
    "lint:fix": "yarn lint -- --fix",
    "prettier": "prettier 'packages/**' 'examples/**' 'benchmarks/**' 'scripts/**' --write --ignore-unknown",
    "prettier:ci": "yarn prettier --check",
    "release": "node scripts/packaging.js publish",
    "clean": "yarn workspaces foreach exec rm -rf dist",
    "benchmark": "concurrently -k --kill-others-on-fail -s first \"node benchmarks/dist/createServer.js\" \"node -e 'setTimeout(() => {}, 1000)' && node benchmarks/dist/index.js\"",
    "postinstall": "husky install",
    "release:prepare": "node scripts/packaging.js prepare",
    "release:publish": "node scripts/packaging.js publish"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorsalgado/drizzle-http.git"
  },
  "author": {
    "name": "Vitor Hugo Salgado",
    "url": "https://github.com/vitorsalgado"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vitorsalgado/drizzle-http/issues"
  },
  "homepage": "https://github.com/vitorsalgado/drizzle-http",
  "workspaces": [
    "benchmarks",
    "internal/*",
    "packages/*",
    "examples/*",
    "test"
  ],
  "keywords": [
    "http",
    "http client",
    "rest",
    "api",
    "undici",
    "node",
    "typescript",
    "decorators",
    "easy",
    "annotations"
  ],
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@jest/types": "^27.4.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/benchmark": "^2.1.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.6",
    "@types/node-fetch": "^3.0.3",
    "@types/opossum": "^6.2.0",
    "@types/pino": "^7.0.5",
    "@types/puppeteer": "^5.4.4",
    "@types/superagent": "^4.1.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "clean-webpack-plugin": "^4.0.0",
    "codecov": "^3.8.3",
    "commander": "^8.3.0",
    "concurrently": "^7.0.0",
    "dotenv": "^14.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fastify": "^3.25.3",
    "fastify-formbody": "^5.2.0",
    "fastify-multipart": "^5.2.1",
    "fs-extra": "^10.0.0",
    "globby": "^12.0.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lerna": "^4.0.0",
    "lint-staged": "^12.1.5",
    "pino": "^7.6.2",
    "pino-pretty": "^7.3.0",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "rollup": "^2.62.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "10.4.0",
    "typescript": "^4.5.4",
    "undici": "^4.12.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  },
  "packageManager": "yarn@3.1.1"
}
