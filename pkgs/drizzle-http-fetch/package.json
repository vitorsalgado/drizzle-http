{
  "name": "@drizzle-http/fetch",
  "version": "2.2.0",
  "description": "Browser Fetch HTTP client for Drizzle-HTTP",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "pretest": "npm run build:test",
    "build": "rm -rf dist && tsc --build --force tsconfig.build.json && npm run build:test",
    "build:test": "webpack --config ./build/webpack.config.cjs",
    "test": "concurrently -k --kill-others-on-fail -s first \"node __tests__/server.js\" \"node __tests__/serverStatic.js\" \"node -e 'setTimeout(() => {}, 1000)' && jest __tests__/fetch.spec.ts\"",
    "install:puppeteer": "node_modules/puppeteer/install.js",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "author": {
    "name": "Vitor Hugo Salgado",
    "url": "https://github.com/vitorsalgado"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/vitorsalgado/drizzle-http/tree/main/pkgs/drizzle-http-fetch",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorsalgado/drizzle-http.git",
    "directory": "pkgs/drizzle-http-fetch"
  },
  "bugs": {
    "url": "https://github.com/vitorsalgado/drizzle-http/issues"
  },
  "keywords": [
    "drizzle-http",
    "fetch",
    "browser",
    "http",
    "api"
  ],
  "dependencies": {
    "@drizzle-http/core": "^2.2.0"
  }
}
