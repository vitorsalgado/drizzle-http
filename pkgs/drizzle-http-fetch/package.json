{
  "name": "@drizzle-http/fetch",
  "version": "0.0.1",
  "description": "Browser Fetch HTTP client for Drizzle-HTTP",
  "browser": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "pretest": "yarn build",
    "build": "tsc --build && webpack --config webpack.config.js && rm -rf ./dist/test",
    "postinstall": "yarn build",
    "test": "concurrently -k --kill-others-on-fail -s first \"node __tests__/server.js\" \"node src/test/server_static.ts\" \"node -e 'setTimeout(() => {}, 1000)' && jest src/fetch.spec.ts\""
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Vitor Hugo Salgado",
    "url": "https://github.com/vitorsalgado"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorsalgado/drizzle-http.git",
    "directory": "pkgs/drizzle-http-fetch"
  },
  "keywords": [
    "drizzle-http",
    "fetch",
    "browser",
    "http",
    "api"
  ],
  "dependencies": {
    "@drizzle-http/core": "0.0.1",
    "bops": "^1.0.1",
    "querystring-es3": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-decorators": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.2",
    "@babel/preset-env": "^7.13.0",
    "@babel/runtime": "^7.13.2",
    "@drizzle-http/test-utils": "0.0.1",
    "@types/puppeteer": "^5.4.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.0",
    "puppeteer": "^7.1.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0"
  }
}
