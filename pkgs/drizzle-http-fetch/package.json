{
  "name": "@drizzle-http/fetch",
  "version": "2.2.0",
  "description": "Browser Fetch HTTP client for Drizzle-HTTP",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "pretest": "npm run build:test",
    "build": "rm -rf dist tsconfig.build.tsbuildinfo && tsc --build --force tsconfig.build.json && npm run build:test",
    "build:test": "webpack --config ./build/webpack.config.js && rm -rf ./dist/test",
    "test": "concurrently -k --kill-others-on-fail -s first \"node src/__tests__/server.js\" \"node src/__tests__/serverStatic.js\" \"node -e 'setTimeout(() => {}, 1000)' && jest src/__tests__/fetch.spec.ts\"",
    "install:puppeteer": "node_modules/puppeteer/install.js",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "author": {
    "name": "Vitor Hugo Salgado",
    "url": "https://github.com/vitorsalgado"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/vitorsalgado/drizzle-http/tree/main/pkgs/drizzle-http-fetch",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorsalgado/drizzle-http.git",
    "directory": "pkgs/drizzle-http-fetch"
  },
  "bugs": {
    "url": "https://github.com/vitorsalgado/drizzle-http/issues"
  },
  "keywords": [
    "drizzle-http",
    "fetch",
    "browser",
    "http",
    "api"
  ],
  "dependencies": {
    "@drizzle-http/core": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/runtime": "^7.16.3",
    "@drizzle-http/test-utils": "^2.2.0",
    "@types/puppeteer": "^5.4.4",
    "babel-loader": "^8.2.3",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.4.0",
    "puppeteer": "^12.0.0",
    "ts-loader": "^9.2.6",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  }
}
