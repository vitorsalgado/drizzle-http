{
  "name": "drizzle-http",
  "version": "2.2.0",
  "description": "Drizzle-HTTP default packages for Node.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf dist tsconfig.build.tsbuildinfo && tsc --build --force tsconfig.build.json",
    "lint": "eslint --fix --ext .ts .",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16"
  },
  "author": {
    "name": "Vitor Hugo Salgado",
    "url": "https://github.com/vitorsalgado"
  },
  "license": "MIT",
  "homepage": "https://github.com/vitorsalgado/drizzle-http",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitorsalgado/drizzle-http.git"
  },
  "bugs": {
    "url": "https://github.com/vitorsalgado/drizzle-http/issues"
  },
  "keywords": [
    "drizzle-http",
    "http",
    "http client",
    "rest",
    "api",
    "undici",
    "node",
    "typescript",
    "decorators",
    "easy",
    "annotations",
    "feign",
    "retrofit"
  ],
  "dependencies": {
    "@drizzle-http/core": "^2.2.0",
    "@drizzle-http/logging-interceptor": "^2.2.0",
    "@drizzle-http/opossum-circuit-breaker": "^2.2.0",
    "@drizzle-http/response-mapper-adapter": "^2.2.0",
    "@drizzle-http/rxjs-adapter": "^2.2.0",
    "@drizzle-http/undici": "^2.2.0"
  }
}
