{
  "version": "0.0.0",
  "npmClient": "npm",
  "packages": [
    "benchmarks",
    "pkgs/*",
    "examples/*"
  ],
  "ignoreChanges": [
    "docs",
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md"
  ],
  "command": {
    "publish": {
      "ignoreChanges": [
        "docs",
        "*.md"
      ],
      "message": "chore(release): publish %s"
    }
  }
}
